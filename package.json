{
  "name": "mosaic",
  "main": "index.ts",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:ci": "jest",
    "lint": "exec biome lint --changed",
    "format": "exec biome format --write .",
    "lint-format": "exec biome check --write",
    "prepare": "simple-git-hooks",
    "build:dev": "eas build --platform ios --profile development",
    "build:preview": "eas build --platform ios --profile preview",
    "build:prod": "eas build --platform ios --profile production"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|expo(nent)?|@expo(nent)?/.*)"
    ]
  },
  "resolutions": {
    "pretty-format": "29.7.0"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/drawer": "^7.5.9",
    "@react-navigation/native": "^7.1.6",
    "expo": "^54.0.12",
    "expo-dev-client": "^6.0.20",
    "expo-font": "~14.0.8",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.8",
    "expo-router": "~6.0.10",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.8",
    "i18next": "^25.7.4",
    "install": "^0.13.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.5.3",
    "react-native": "0.81.4",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^4.1.1",
    "react-native-nitro-modules": "^0.33.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-unistyles": "^3.0.21",
    "react-native-web": "^0.21.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "2.2.5",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.10",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.12",
    "lint-staged": "^16.1.4",
    "react-native-worklets": "0.5.1",
    "react-test-renderer": "19.1.0",
    "simple-git-hooks": "^2.13.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
